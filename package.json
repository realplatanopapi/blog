{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "chakra": "chakra",
    "db:generate": "pnpm lint:prisma && prisma generate",
    "db:migrate": "pnpm lint:prisma && prisma migrate dev",
    "db:studio": "pnpm lint:prisma && prisma studio",
    "dev": "concurrently -k -c 'cyan,magenta,gray' -n 'app,theme,docker' 'pnpm dev:app' 'pnpm dev:theme' 'pnpm dev:docker'",
    "dev:app": "next dev",
    "dev:theme": "chakra typegen ./src/theme/system.ts --watch",
    "dev:docker": "docker compose up",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm lint:prisma && pnpm lint:ts",
    "lint:prisma": "prisma-lint prisma/schema/",
    "lint:ts": "eslint --cache",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.8.1",
    "@emotion/react": "^11.14.0",
    "@prisma/client": "^6.4.1",
    "@tailwindcss/postcss": "^4.0.8",
    "envalid": "^8.0.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "postcss": "^8.5.3",
    "prisma": "^6.4.1",
    "prisma-lint": "^0.8.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.0.8"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.8.1",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "typescript": "^5"
  }
}
